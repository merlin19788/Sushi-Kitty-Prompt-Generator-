    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 20px;
            background-color: black;
            text-align: center;
        }

        h1 {
            color: white;
        }
        #backgroundVideo {
            position: fixed;
            right: 0;
            bottom: 0;
            min-width: 100%;
            min-height: 100%;
            width: auto;
            height: auto;
            z-index: -1;
        }

        .dropdown {
            display: inline-block;
            margin: 10px;
        }

        .dropbtn {
            background-color: rgba(0, 0, 0, 0); /* Transparent background */
            color: white;
            padding: 10px;
            font-size: 16px;
            border: 1px solid black; /* Black border */
            cursor: pointer;
        }

        .dropdown-content {
            display: none;
            position: absolute;
            background-color: rgba(0, 0, 0, 0.9); /* Semi-transparent white background */
            min-width: 160px;
            box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2);
            z-index: 1;
        }

        .dropdown-content a {
            color: white; /* White text */
            padding: 12px 16px;
            text-decoration: none;
            display: block;
            border-bottom: 1px solid black; /* Black border between items */
        }

        .dropdown-content a:last-child {
            border-bottom: none; /* Remove border from the last item */
        }

        .dropdown-content a:hover {
            background-color: #ddd;
        }

        .dropdown:hover .dropdown-content {
            display: block;
        }

        #buttonContainer {
            margin-top: 20px;
        }

        #selected-options {
            margin-top: 20px;
            padding: 10px;
            border: 1px solid #ccc;
            display: inline-block;
        }

        #editableTextarea {
            width: 355px;
            height: 153px;
        }
    </style>
</head>
