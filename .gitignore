        var button = document.getElementById('popupButton');
        var video = document.getElementById('myVideo');
        var fadingText = document.getElementById('fadingText');
        var textArray = [
            { text: "Sushi Kitty integrates with leading A.I. art programs like ComfyUI and Stable Diffusion Online.", color: "#FFFFFF" },
            { text: "Unleash your storytelling prowess with handpicked prompts that captivate and inspire.", color: "#FFFFFF" },
            { text: "Discover the art of storytelling with prompts that beckon your imagination to take flight.", color: "#FFFFFF" },
            { text: "Images based on text prompts Sometimes yield precise and desired outputs, Others may result in suboptimal images.", color: "#FFFFFF" },
            { text: "Try our Random Prompt Generator for inspiration in creating the perfect prompt.", color: "#FFFFFF" },
            { text: "Transform your ideas into narratives with a touch of AI magic and our curated prompts.", color: "#FFFFFF" },
            { text: "Utilize our dropdown menus to select the appropriate prompt style.", color: "#FFFFFF" },
            { text: "Embrace the challenge of new prompts – where every word is a step towards brilliance.", color: "#FFFFFF" },
            { text: "A well-crafted prompt can transform your idea into a great image.", color: "#FFFFFF" },
            { text: "A poorly Written Prompt can yeild uncanny-valley monstrosities with too many fingers.", color: "#FFFFFF" },
            { text: "Stable Diffusion tends to thrive on specific prompts, especially when compared to something like MidJourney.", color: "#FFFFFF" },
            { text: "You need to tell it exactly what you want.", color: "#FFFFFF" },
            { text: "Some words and phrases tend to more strongly bias an image than other words, so you might need to adjust your prompt accordingly.", color: "#FFFFFF" },
            { text: "You can (and should) also specify the style you want.", color: "#FFFFFF" },
            { text: "A.I Art Gens recognize different styles, everything from pencil drawings to clay models to 3d rendering from Unreal Engine.", color: "#FFFFFF" },
            { text: "You can use dozens of keywords to fine-tune your results.", color: "#FFFFFF" },
            { text: "If you want weird, psychedelic-colored clay chicken models in a Walmart, you need to specify all those terms.", color: "#FFFFFF" },
            { text: "A.I. art generators can emulate styles, mediums, and even mimic specific artists.", color: "#FFFFFF" },
            { text: "You should definitely try throwing by (Artist Name) into your prompts.", color: "#FFFFFF" },
            { text: "Don't be afraid to combine artists that wouldn't normally go together, either.", color: "#FFFFFF" },
            { text: "You can tell it that it really needs to pay attention to a specific keyword or keywords and pay less attention to others.", color: "#FFFFFF" },
            { text: "You can use multiple parentheses to stack up the emphasis as much as you want.", color: "#FFFFFF" },
            { text: "As a rule, Stable Diffusion creates sunsets and fall leaves exceptionally well.", color: "#FFFFFF" },
            { text: "(Subject- anything), 35mm, sharp, this prompt reliably makes photorealistic results of people, animals, and landscapes.", color: "#FFFFFF" },
            { text: "(Subject- anything), low poly 3d render, vibrant pastel colors, tilt shift, film grain.", color: "#FFFFFF" },
            { text: "Certain words, like gorgeous, will heavily influence what an image looks like, even if it doesn't have a precise meaning.", color: "#FFFFFF" },
            { text: "You'll quickly find that writing a good prompt is an art unto itself.", color: "#FFFFFF" },
            // Add more texts as needed
        ];
        var textIndex = 0;

        function togglePopup() {
            popup.style.display = (popup.style.display === 'none' || popup.style.display === '') ? 'block' : 'none';
        }

        function closePopup(event) {
            if (event.target === popup || event.target.classList.contains('close-button')) {
                togglePopup();
            }
        }

        button.addEventListener('click', togglePopup);
        window.addEventListener('click', closePopup);

        // Adjust the size of the video dynamically based on the window width
        function adjustVideoSize() {
            var windowWidth = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
            video.style.width = windowWidth > 600 ? '600px' : '100%'; // Adjust the width as needed
        }

        // Function to fade in/out and change text
           function fadeText() {
           fadingText.style.opacity = 0;
           fadingText.innerHTML = textArray[textIndex].text;  // Use innerHTML instead of innerText
           fadingText.style.color = textArray[textIndex].color;
           textIndex = (textIndex + 1) % textArray.length;
           setTimeout(function () {
            fadingText.style.opacity = .5;
           }, 500); // Adjust the fade duration as needed
        }

        // Call the adjustVideoSize function on page load and window resize
        window.addEventListener('load', adjustVideoSize);
        window.addEventListener('resize', adjustVideoSize);

        // Call the fadeText function to start the text fading
        setInterval(fadeText, 9000); // Adjust the interval between text changes as needed
    });
</script>
