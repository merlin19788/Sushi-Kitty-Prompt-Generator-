<script>
    function updateDisplay(option, menuNumber) {
        var editableTextarea = document.getElementById('editableTextarea');
        var currentOptions = editableTextarea.value.split('\n');
        currentOptions[menuNumber - 1] = option;
        editableTextarea.value = currentOptions.join(''); // Separate options without a comma
    }

